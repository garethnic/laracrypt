{
    "name": "garethnic/laracrypt",
    "type": "library",
    "description": "Add database encryption to laravel",
    "keywords": [
        "garethnic",
        "laracrypt",
        "encryption",
        "laravel"
    ],
    "homepage": "https://github.com/garethnic/laracrypt",
    "license": "MIT",
    "authors": [
        {
            "name": "Gareth Nicholson",
            "email": "gareth.nic@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~7.0",
        "paragonie/halite": "^1.5",
        "ext-libsodium": "^1.0.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "garethnic\\laracrypt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "garethnic\\laracrypt\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
